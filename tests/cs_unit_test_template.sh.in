#!/bin/bash

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:@LIBRARY_PATH@
export LIBPKMN_DATABASE_PATH=@DATABASE_PATH@
export LIBPKMN_IMAGES_DIR=@IMAGES_PATH@
export PATH=$PATH:@LIBRARY_PATH@
export PYTHONPATH=$PYTHONPATH:@PYTHON_PATH@

if [ ! -f "@LIBPKMN_BINARY_DIR@/tests/libpkmn-cs.dll" ]
then
    cp @LIBRARY_PATH@/libpkmn-cs.dll @CMAKE_CURRENT_BINARY_DIR@
fi

@CSHARP_INTERPRETER@ @CMAKE_CURRENT_BINARY_DIR@/@TEST_NAME@.exe
